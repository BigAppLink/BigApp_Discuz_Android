<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="WithRatingBarTextView">

        <!-- 控件背景 -->
        <attr name="WithRatingBarTextView_background" format="reference" />

        <!-- 标题文字 -->
        <attr name="WithRatingBarTextView_title" format="string" />

        <!-- 标题文字 -->
        <attr name="WithRatingBarTextView_title_size" format="dimension" />

        <!-- 标题文字颜色 -->
        <attr name="WithRatingBarTextView_title_color" format="color" />

        <!-- 从activity带回的文字 -->
        <attr name="WithRatingBarTextView_content" format="string" />

        <!-- 标题文字 -->
        <attr name="WithRatingBarTextView_content_size" format="dimension" />

        <!-- 从activity带回的文字颜色 -->
        <attr name="WithRatingBarTextView_content_color" format="color" />

        <!-- 从activity带回的文字,是否居左 -->
        <attr name="WithRatingBarTextView_is_content_text_left" format="boolean" />



        <!-- 控件背景 -->
        <attr name="WithRatingBarTextView_ratingbar_progressDrawable" format="reference" />
        <!-- 控件背景 -->
        <attr name="WithRatingBarTextView_ratingbar_num" format="integer" />


        <!-- 标题文字 -->
        <attr name="WithRatingBarTextView_margin" format="dimension" />
        <!-- 标题文字 -->
        <attr name="WithRatingBarTextView_margin_left" format="dimension" />
        <!-- 标题文字 -->
        <attr name="WithRatingBarTextView_margin_right" format="dimension" />

    </declare-styleable>

</resources>