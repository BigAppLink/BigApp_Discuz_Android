<?xml version="1.0" encoding="UTF-8"?>
<resources>
    <declare-styleable name="RoundProgressBar">  
        <attr name="RoundProgressBar_roundColor" format="color"/>
        <attr name="RoundProgressBar_roundProgressColor" format="color"/>
        <attr name="RoundProgressBar_roundWidth" format="dimension"></attr>
        <attr name="RoundProgressBar_textColor" format="color" />
        <attr name="RoundProgressBar_textSize" format="dimension" />
        <attr name="RoundProgressBar_max" format="integer"></attr>
        <attr name="RoundProgressBar_textIsDisplayable" format="boolean"></attr>
        <attr name="RoundProgressBar_style">
            <enum name="STROKE" value="0"></enum>
            <enum name="FILL" value="1"></enum>
        </attr>
    </declare-styleable> 
</resources>